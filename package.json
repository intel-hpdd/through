{
  "name": "@mfl/through",
  "version": "2.0.0",
  "description": "A collection of through modules for highland",
  "main": "source/index.js",
  "author": "MFL Team",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "eslint": "eslint ./",
    "flow": "flow",
    "cover": "NODE_ENV=test npmt t -- --coverage"
  },
  "pre-commit": [
    "eslint",
    "flow",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/intel-hpdd/through.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "resetModules": true,
    "resetMocks": true,
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@mfl)/"
    ]
  },
  "dependencies": {
    "@mfl/fp": "8.0.2",
    "@mfl/obj": "6.0.3",
    "highland": "3.0.0-beta.4",
    "lodash.isequal": "4.5.0"
  },
  "devDependencies": {
    "@mfl/flow-highland": "4.2.6",
    "@mfl/flow-jasmine": "1.6.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-object-values-to-object-keys": "1.0.2",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-env": "1.4.0",
    "eslint": "3.19.0",
    "eslint-config-prettier": "2.0.0",
    "eslint-plugin-flowtype": "2.32.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-prettier": "2.0.1",
    "flow-bin": "0.46.0",
    "jest": "20.0.1",
    "pre-commit": "1.2.2",
    "prettier": "1.3.1"
  }
}
